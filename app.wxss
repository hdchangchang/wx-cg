/**app.wxss**/

@import './icon.wxss';

div,ul,li,p,h1,h2,h3,h4,h5,h6,img{
  display: block;
  box-sizing: border-box;
}
span,a,i{
  display: inline-block
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.pop{
  position: fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  z-index:100;
  background: rgba(0,0,0,.5)
}


/* 标签 */
.good-lab span{
  display: inline-block;
  font-size: 20rpx;
  padding: 2rpx 10rpx;
  border:1px solid orange;
  color:orange;
  border-radius: 25rpx;
  margin-right: 8rpx;
}