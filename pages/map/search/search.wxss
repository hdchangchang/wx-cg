/* pages/map/search/search.wxss */

.search-txtbox{
  position: relative;
  box-sizing: border-box;
  height:90rpx;
  width:94%;
  margin:auto;
  border:1px solid #ccc;
  font-size: 26rpx;
  border-radius: 10rpx;
  padding-left:140rpx;
  background: #f8f8f8f8;
}
.search-city{
  position: absolute;
  width:140rpx;
  line-height: 88rpx;
  text-align: center;
  top:0;
  left:0;
  font-size: 32rpx;
}
.search-txt{
  box-sizing: border-box;
  width:100%;
  line-height:50rpx;
  margin-top:20rpx;  
  padding:0 20rpx;
  border-left:1px solid #ccc;
  padding-right: 160rpx;
}
.search-btn{
  width:90rpx;
  height:90rpx;
  line-height:88rpx;
  position: absolute;
  top:0rpx;
  right:0rpx;
  text-align: center;
}

.search-list{
  margin: 30rpx; 
}

.search-list-li{
  padding:26rpx 0;
  color: #333;
  border-bottom:1px dashed #e1e1e1;
  font-size: 28rpx;
}

.search-list-li p{
  color: #999;
  font-size: 22rpx;
  margin-top:10rpx;
}

.search-his{
  
  padding:30rpx;
}
.his-tit{
  font-size: 28rpx;
  height: 90rpx;
  line-height:90rpx;
}
.his-labbox{
  display: flex;
}
.his-labbox span{
  font-size: 24rpx;
  border:1px solid #e1e1e1;
  margin-right: 30rpx;
  padding:10rpx 30rpx;
}